@charset "utf-8";

/* webkit内核 模拟滚动条 */
::-webkit-scrollbar{padding-left:1px; background-color:#fff; overflow:visible; width:13px; height:13px; }
::-webkit-scrollbar-thumb{ background-color:rgba(0, 0, 0, .2); background-clip:padding-box; border:2px solid transparent;  min-height:15px; box-shadow:inset 1px 1px 0 rgba(0, 0, 0, .1),inset 0 -1px 0 rgba(0, 0, 0, .07); border-radius:12px;}
::-webkit-scrollbar-thumb:vertical:hover{background-color:rgba(0, 0, 0, .3); border:2px solid transparent;}
::-webkit-scrollbar-thumb:vertical:active{background-color:rgba(0, 0, 0, .5); border:2px solid transparent;}
::-webkit-scrollbar-button{height:0; width:0;}
::-webkit-scrollbar-track{background-clip:padding-box; border:solid transparent; border-width:0 0 0 4px; border-radius:0;}
::-webkit-scrollbar-corner{background:transparent;}

/* 默认图片拉伸 */
img{ max-width: 100%;}

/* 框架 */
body#xbody { background: #f8f8f8; color: #666; }

/* 屏蔽表单在框架内底图背景*/
body#xbody input[type="text"] {background:none;}

/* webkit内核 屏蔽表单黄色区域 */
input:-webkit-autofill { -webkit-box-shadow: 0 0 0 1000px white inset;}

