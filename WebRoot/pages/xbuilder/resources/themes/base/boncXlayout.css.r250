.phoneGuideBg{ background:url("images/bg_phone_guide.png") 0 0 repeat!important;}
.pc_guide .phoneDragger { display:none;}
.phoneDragger:link, .phoneDragger:hover:visited { overflow:visible; background:url(images/icoPhoneDragger.png) 0 50% no-repeat #eee; border:1px solid #b5b5b5; border-right:0; filter:alpha(opacity=70); -moz-opacity:0.7; -khtml-opacity: 0.7; opacity: 0.7; width:16px; height:33px; position:fabsolute; right:-14px; z-index:9999999;  cursor:e-resize; top:50%; margin-top:-16px; text-indent:-5000px;}
.phoneDragger:hover, .phoneDragger:focus, .phoneDragger:active{display:block; overflow:visible; background:url(images/icoPhoneDragger.png) 0 50% no-repeat #eee; border:1px solid #b5b5b5; border-right:0;width:16px; height:33px; position:absolute; right:0;  cursor:e-resize; top:50%; margin-top:-16px; text-indent:-5000px;filter:alpha(opacity=100); -moz-opacity:1; -khtml-opacity:1; opacity:1;}

.charInner .inbox{ border: 1px solid #e6e6e6; float: left; position: relative; margin: 5px;}
.inbox .curB01,
.inbox .curB02,
.inbox .curB03,
.inbox .curB04{ display: block; visibility: hidden; position: absolute; width: 6px; height: 6px; font-size: 0; line-height: 0; background: #000000 }
.inbox .curB01{ left: -3px; top: -3px;}
.inbox .curB02{ right: -3px; top: -3px;}
.inbox .curB03{ left: -3px; bottom: -3px;}
.inbox .curB04{ right: -3px; bottom: -3px;}
.charInner .inbox.cur{ border: 1px solid #000; }
.inbox.cur .curB01,
.inbox.cur .curB02,
.inbox.cur .curB03,
.inbox.cur .curB04{ visibility: visible;}

/* 系统主体-指标库 */
.comOutBox{ margin:10px;}
.comOutBox .titOut{ background:#fafafa; line-height:36px; height:38px!important; margin-bottom:10px; }
.comOutBox .titOut h3 {font-weight:normal; display:block; height:38px; padding:0; background:url("images/bg_tabLine.png") left bottom repeat-x; color:#199ed8; }
.comOutBox .titOut h3 span{ display:block; float:left; padding:0 12px 0 12px; height:36px; font-size:16px;  border-bottom:2px solid #7bd101;}
.comOutBox .comInbox{ padding-bottom:10px;}
.comOutBox .comInbox dl{ position: relative; padding:5px 5px 5px 70px; magin-top:10px;}
.comOutBox .comInbox dl dt{position: absolute; top:50px; left:0; width:65px; text-align: right;}
.comOutBox .comInbox dl dd{widht:100%;}
.comOutBox .comInbox dl dt.topLine{ top:6px;}
.comOutBox .comInbox dl dd span.pl6{ padding-left:6px}
.comOutBox .comInbox dl dd .formEdit{width:90%;height:100px; overflow:auto; border:1px solid #d3cfc8; padding: 10px 10px 10px 10px;}
.comOutBox .comInbox p.titleSearch{ margin:5px; line-height: 1.8}
.comOutBox .comInbox input[type="text"]{ height:22px;}
.comOutBox .comInbox .delBtnB { margin-right:5px;  display:inline-block; border:none; background: #dedede; border-radius:2px; -webkit-border-radius:2px;  line-height:1.8; color:#333333; padding:0 6px; }
.comOutBox .comInbox .delBtnB:hover{ background: #199ed8; color:#fff;}
.condDiv{width:100%;height:200px; overflow:auto;border:1px solid #fff; margin-bottom:10px;}
.newAddkpi.easyui-panel{border: 0;}
.newAddkpi .panel-title{font-size: 14px;}

.radioGroup {padding:10px 0 0 5px;}
.radioGroup a{ width:42px; height:20px;; float:left; text-align:center; line-height:20px; border:1px solid #d3cfc8; color:#888;  font-size: 14px; text-decoration: none; }
.radioGroup a.cur{ background: #199ed8; color:#fff; border:1px solid #199ed8;}
.checkboxGroup{ padding:10px 0 0 5px;}
.checkboxGroup li{ margin-top: 10px;}
.checkboxGroup li span{ padding-left: 5px; color: #666; font-size: 14px; }

/* 页脚 */
#footer { position: fixed; left: 0; bottom: 0; z-index: 99; background: url(images/background/bg_footer.gif) 0 bottom repeat-x; height: 24px; line-height:20px; text-align: center; width: 100%;  color: #fff; }

/* 系统主体-其他 */
.checkboxGroup1 p { background:#199ed8; color:#fff; padding:2px 2px 2px 20px; }
.checkboxGroup1 ul {display:block;}
.checkboxGroup1 ul li {display:block; padding:2px 2px 2px 20px; display:block; background:#fafafa; margin-top:2px; cursor:move;}
.checkboxGroup1 ul li:hover { background:#ddd;}

.gridster>ul { position:relative; width:inherit; height:inherit;}

/* 生产页面 页面描述 */
.HBcomments { margin:20px 2px; }
.HBcomments h3 { display:block; font-size: 16px; margin-top:20px; line-height:1.2;  border-bottom:2px solid #199ed8; position:relative;  font-weight:normal; color:#199ed8;  }
.HBcomments h3 span { margin-bottom:10px; margin-left:15px; display:block;}
.HBcomments h4 { color:#333; font-size:14px; margin-top:10px; margin-left:15px; font-weight:normal;}
.HBcommentsDate { border:1px solid #ddd; text-align:center; line-height:2; margin-top:10px; width:100%;}
.HBcommentsDate th { border:1px solid #ddd; border-right-style:dotted;border-left-style:dotted; background:#fff; font-weight:normal; color:#666;}
.HBcommentsDate td { border:1px solid #ddd;border-right-style:dotted; border-left-style:dotted;}
.describeBox { margin-top:5px; background:#fff; min-height:50px; padding:10px;}

.loadBox {width:100%; height:100%;}

