#boncFrame #mainMenuSimple .mainMenu li.icoN10 a { background-image:url("images/boncLayout/bg/chinaTower_Ico10.png");}

/* bonc框架 > 缩略图主菜单~一级链接状态 */
#boncFrame #mainMenuSimple .mainMenu li a:hover,
#boncFrame #mainMenuSimple .mainMenu li a:active,
#boncFrame #mainMenuSimple .mainMenu li a:focus,
#boncFrame #mainMenuSimple .mainMenu li a.selected { background-position: 50% -50px; transition:background .5s; background-color:#293846;  }

/* bonc框架 > 缩略图主菜单~切换按钮 */
#boncFrame #mainMenuSimple .layout-button-right { background:transparent;}
#boncFrame #mainMenuSimple .easyui-linkbutton.l-btn-small.easyui-fluid { border:0; width:68px!important; border-radius:0; height:60px!important; background:url("images/boncLayout/bg/chinaTower_icon_plus.png") 50% 50% no-repeat #367fa9; opacity:1;}

/* 主体css特效 */
@-webkit-keyframes fadeInLeft{0%{opacity:0;-webkit-transform:translateX(-20px)}100%{opacity:1;-webkit-transform:translateX(0)}}
@-moz-keyframes fadeInLeft{ 0%{opacity:0; -moz-transform:translateX(-20px)} 100%{opacity:1; -moz-transform:translateX(0)}}


/* ---------------------------------- 基础主界面 ---------------------------------- */

/* bonc主界面 > 顶部信息 */
.IndexHead{ position:fixed;top:0;z-index:100; overflow:hidden;width:100%;padding-top:14px;background:#367fa9;height:60px;}
.IndexHead h1{float:left; margin-left:20px;width:157px; height:40px; background:url("images/boncLayout/bg/boncFrameLogo.png") 0 50% no-repeat;text-indent:-5000px;}
.IndexHead span{float:left;color:#fff;font-size:24px;line-height:40px;border-left:1px solid #96d4e3; padding-left:10px;}

/* bonc主界面 > 背景切换基础 */
.swiper-container {position:absolute !important;top:0;z-index:1;width: 100%;height: 100%;background:#2b363d; }
.swiper-slide { text-align: center;font-size: 18px;background: #fff; /* Center slide text vertically */display: -webkit-box;display: -ms-flexbox;display: -webkit-flex;display: flex;
    -webkit-box-pack: center;-ms-flex-pack: center;-webkit-justify-content: center;justify-content: center;-webkit-box-align: center;-ms-flex-align: center;-webkit-align-items: center;align-items: center;}
.swiper-wrapper .slide01,
.swiper-wrapper .slide02,
.swiper-wrapper .slide03{ width:100%; height:100%; background-size:100% 100% !important;}

/* bonc主界面 > 背景切换图标基础 */
.swiper-wrapper .slide01{ background:url("images/boncLayout/img/img01_swiper.jpg") no-repeat;}
.swiper-wrapper .slide02{ background:url("images/boncLayout/img/img02_swiper.jpg") no-repeat;}
.swiper-wrapper .slide03{ background:url("images/boncLayout/img/img03_swiper.jpg") no-repeat;}


/* bonc主界面 > 基础状态 */
.ContentIndex{overflow:hidden;position:absolute;top:50%;left:50%;z-index:200;width:1140px;height:300px; margin-left:-570px;margin-top:-150px;}
.ContentIndex a{float:left;margin:0  0 5px 5px;text-decoration:none; transition:.2s all ease;}
.ContentIndex a:hover{cursor:pointer; }
.ContentIndex a span{padding:10px;line-height:40px;color:#fff; font-size:16px;opacity:0.8;}
.ContentIndex .item01, .ContentIndex .item02{float:left;overflow:hidden; height:300px;}
.ContentIndex .item01{width:150px;}
.ContentIndex .item02{width:310px;}

